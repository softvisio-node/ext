{
    "framework": "ext",
    "toolkit": "modern",
    "requires": [
        "froala-editor",
        "charts"
    ],
    "overrides": [
        "overrides"
    ],
    "packages": {
        "dir": [
            "../../node_modules/@sencha",
            "../ext-addons/packages"
        ]
    },
    "language": {
        "js": {
            "input": "ES8",
            "output": "ES8"
        }
    },
    "output": {
        "js": {
            "version": "ES8"
        },
        "base": ".",
        "resources": {
            "path": "./resources",
            "shared": "./resources"
        }
    },
    "theme": "theme-material"
}
